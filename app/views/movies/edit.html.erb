<h1>Hi Edit Page</h1>

<h2>You are viewing  <%=  @movie.title %> </h2>

<%= form_with(model: @movie, local: true, data: { turbo: false }) do |f| %>

<p>
<%= f.label :view_count, "Enter a views" %><br/>
<%= f.text_field :view_count , value: @movie.view_count %>
</p>

  <p>
    <%= f.submit %>
  </p>

  <% end %>